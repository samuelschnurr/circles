@attribute [Route(Routes.Circle.Page)]
@using Io.Schnurr.Circles.Shared

<PageTitle>Circle</PageTitle>

<MudGrid>
    @foreach (var circle in circles)
    {
        <MudItem xs="12" lg="6">
            <PaperSwitch Label="@circle.label" Checked="circle.isChecked" Disabled="circle.isDisabled" ReadOnly="circle.isReadOnly" />
        </MudItem>
    }
</MudGrid>